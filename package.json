{
  "name": "kubota_pos",
  "version": "1.0.0",
  "description": "Point of sale management system for kubota product",
  "main": "main.js",
  "build": {
    "appId": "com.kubota.desktop",
    "productName": "ArgriTech POS",
    "copyright": "InnoScript Co.,Ltd",
    "buildVersion": "1",
    "directories": {
      "app": ".",
      "output": "output",
      "buildResources": "build-res"
    },
    "files": [
      "./build/**/*",
      "./src/**/*",
      "./main.js",
      "./preload.js",
      "./index.html"
    ],
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "watch": "webpack --config webpack.common.js --watch",
    "pack": "electron-builder --linux",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "react-build": "webpack --config webpack.common.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubota-POS/desktop.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kubota-POS/desktop/issues"
  },
  "homepage": "https://github.com/kubota-POS/desktop#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "express": "^4.17.2",
    "i18next": "^21.6.6",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.4",
    "react-bootstrap-icons": "^1.7.2",
    "react-data-table-component": "^7.4.6",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.5",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "electron-reload": "^2.0.0-alpha.1",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
